{
    "name": "nkt/translate-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides translation mechanism for Doctrine entities",
    "keywords": [
        "translation",
        "translate",
        "entity"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Gusakov",
            "email": "dev@nkt.me"
        }
    ],
    "require": {
        "php": "~5.4",
        "symfony/symfony": "~2.3",
        "doctrine/orm": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Nkt\\TranslateBundle\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
